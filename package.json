{
    "name": "subtle-brackets",
    "displayName": "Subtle Brackets",
    "description": "Underlined matching brackets for VSCode",
    "version": "0.0.1",
    "publisher": "rafamel",
	"repository": {
		"type": "git",
		"url": "https://github.com/rafamel/subtle-brackets"
	},
    "bugs": {
        "url": "https://github.com/rafamel/subtle-brackets/issues"
    },
	"license": "SEE LICENSE IN LICENSE",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": ["bracket", "brackets", "matching", "style"],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": []
